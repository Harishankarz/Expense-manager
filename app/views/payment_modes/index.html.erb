<div class="col-xs-12">
  <div class="card">
    <div class="header">
      <h1> Payment Modes</h1>
      <ul class="header-dropdown m-r--5">
        <li class="dropdown">
          <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="true">
            <i class="material-icons">more_vert</i>
          </a>
          <ul class="dropdown-menu pull-right">
            <li><%= link_to 'Add New payment Mode', new_payment_mode_path, method: :get, class: "waves-effect waves-block" %></li>
          </ul>
        </li>
      </ul>
    </div>

  <div class="body table-responsive">
    <table class="table table-bordered">
    <thead>
    <tr>
      <th>Payment_gateway</th>
      <th>created_at</th>
      <th>updated_at</th>
    </tr>
    </thead>
    <tbody>
    <% @paymentmodes.each do |paymentmode| %>
        <tr>
          <td><%= paymentmode.payment_gateway %></td>
          <td><%= paymentmode.created_at %></td>
          <td><%= paymentmode.updated_at %></td>
          <td><%= link_to 'Destroy', payment_mode_path(paymentmode), method: :delete, :data => {:confirm => 'Are you sure?'} %></td>
        </tr>

    <% end %>

    </tbody>
  </table>
  </div>
</div>
</div>