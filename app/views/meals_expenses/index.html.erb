<div class="col-xs-12">
  <div class="card">
    <div class="header">MealExpenseDetails</div>
    <div class="body">
      <%= form_tag(export_meals_expenses_path(:format => :csv), method: :get) do %>

          <div class="container-fluid">
            <div class="row">
              <div class="col-sm-3">
                From_Date
                <%= text_field_tag 'from_date', nil %>
              </div>

              <div class="col-sm-3">
                To_Date
                <%= text_field_tag 'to_date', nil %>
              </div>

              <div class="col-sm-3">
                <%= submit_tag("Submit", class: "btn btn-primary waves-effect") %>
              </div>

            </div>
          </div>
      <% end %>
    </div>
  </div>
</div>

<script type="text/javascript">
    $(document).ready(function () {
        $("#from_date").datepicker({dateFormat: "yy-mm-dd"});
        $("#to_date").datepicker({dateFormat: "yy-mm-dd"});
    });
</script>