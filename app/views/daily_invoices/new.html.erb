<div class="col-xs-12">
  <div class="card">
    <div class="header"><h3>Expense Register</h3>
      <ul class="header-dropdown m-r--5">
        <li class="dropdown">
          <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="true">
            <i class="material-icons">more_vert</i>
          </a>
          <ul class="dropdown-menu pull-right">
            <li><%= link_to 'Load Previous Data', previous_record_path, remote: :true, method: :get, :class => "waves-effect waves-block" %></li>
          </ul>
        </li>
      </ul>
    </div>

    <div class="body">
      <div class="row">
        <!--Daily Invoices starts-->
        <%= form_for @daily_invoice, html: {id: "daily_invoice_form"} do |f| %>
            <% render partial: 'form', locals: {f: f} %>
        <% end %>
        <!--Daily Invoices ends-->
      </div>
    </div>
  </div>
</div>
</div>


